{
  "name": "server-i",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "removeDist": "node ./tools/deleteOutPath.js",
    "tsBuild": "tsc -p tsconfig.json",
    "start": "npm run tsBuild && node ./es/main.js",
    "docs": "typedoc --out docs src --module commonjs --hideGenerator && node ./tools/replaceDocFileUnderline.js",
    "build": "npm run removeDist && npm run tsBuild && npm run docs",
    "test": "nyc mocha -t 5000",
    "test-nyc": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heifade/server-i.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/heifade/server-i/issues"
  },
  "homepage": "https://github.com/heifade/server-i#readme",
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@types/chai": "^4.0.8",
    "@types/express": "^4.0.39",
    "@types/mocha": "^2.2.44",
    "@types/mysql": "^2.15.2",
    "@types/node-fetch": "^1.6.7",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^4.0.1",
    "mysql-i": "^3.0.12",
    "node-fetch": "^1.7.3",
    "nyc": "^11.3.0",
    "source-map-support": "^0.5.0",
    "supertest": "^3.0.0",
    "ts-node": "^3.3.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2"
  }
}
